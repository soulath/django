{
    "version": 2,
    "builds": [{
        "src": "web/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", 
        "runtime": "3.9.13" 
    }
    },
    {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles_build"
        }
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "web/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}